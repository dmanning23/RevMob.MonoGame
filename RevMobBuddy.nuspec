<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>RevMobBuddy</id>
		<version>1.0.2</version>
		<title>RevMobBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/RevMobBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/RevMobBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>RevMob integration for MonoGame.Android and MonoGame.iOS</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, android, ios, monogame, revmob</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.5"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.5"/>
			</group>

      <group targetFramework="net451">
        <dependency id="MonoGame.Framework.DesktopGL" version="3.5"/>
      </group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="RevMobBuddy.Android\bin\Release\RevMobBuddy.Android.pdb" target="lib\MonoAndroid10\RevMobBuddy.Android.pdb" />
		<file src="RevMobBuddy.Android\bin\Release\RevMobBuddy.Android.dll" target="lib\MonoAndroid10\RevMobBuddy.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="RevMobBuddy.iOS\bin\Release\RevMobBuddy.iOS.pdb" target="lib\Xamarin.iOS10\RevMobBuddy.iOS.pdb" />
		<file src="RevMobBuddy.iOS\bin\Release\RevMobBuddy.iOS.dll" target="lib\Xamarin.iOS10\RevMobBuddy.iOS.dll" />

    <!--DesktopGL-->
    <file src="RevMobBuddy.DesktopGL\bin\Release\RevMobBuddy.DesktopGL.pdb" target="lib\net451\RevMobBuddy.DesktopGL.pdb" />
    <file src="RevMobBuddy.DesktopGL\bin\Release\RevMobBuddy.DesktopGL.dll" target="lib\net451\RevMobBuddy.DesktopGL.dll" />

	</files>
</package>
